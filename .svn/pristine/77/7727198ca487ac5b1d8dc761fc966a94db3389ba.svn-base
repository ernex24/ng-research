/* ==========================================================================
    Carusel de Texto Sector Compa√±ia Tipo de Informe
   ========================================================================== */

.carousel{
  margin: 20px 20px;
}

.carousel-control-prev-icon {
  background-image: url("../../../assets/img/imagenes_comunes/interfaz/right-arrow.svg") !important;
  transform: rotate(180deg);
}

.carousel-control-next-icon {
  background-image: url("../../../assets/img/imagenes_comunes/interfaz/right-arrow.svg") !important;
}

.carousel-control-next, .carousel-control-prev {
  z-index: 1;
  margin-left: 32%;
  margin-right: 32%;
  position: absolute;
  top: 0;
  bottom: 0;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  -ms-flex-pack: center;
  justify-content: center;
  width: 15%;
  text-align: center;
  opacity: .8;
  cursor: pointer;
}

.carousel-control{
  top: 180px;
}

.carousel-indicators {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 0;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-pack: center;
  justify-content: center;
  padding-left: 0;
  margin-right: 15%;
  margin-left: 15%;
  list-style: none;
}

.carousel-indicators li {
  position: relative;
  -ms-flex: 0 1 auto;
  -webkit-box-flex: 0;
  flex: 0 1 auto;
  width: 8px;
  height: 8px;
  margin-right: 3px;
  margin-left: 3px;
  text-indent: -999px;
  background-color: #666666;
  border-radius: 10px;
}

.carousel-indicators .active {
  background-color: #24AEB5;
}

/* Date Picker */
.theme-green .bs-datepicker-head {
  background-color: #24AEB5 !important;
}

.form-control {
  visibility: hidden;
  display: block;
  width: 100%;
  height: 0;
  padding: 0;
  margin: 0;
}

.form-group {
  margin-bottom: 0;
}

.theme-green .bs-datepicker-head {
  background-color: #37c1c8;
}

.theme-green .bs-datepicker-body table td span.selected, .theme-green .bs-datepicker-body table td.selected span,
.theme-green .bs-datepicker-body table td span[class*="select-"]:after,
.theme-green .bs-datepicker-body table td[class*="select-"] span:after {
  background-color: #37c1c8;
}

.theme-green .bs-datepicker-body table td.week span {
  color: #37c1c8;
}

.bs-datepicker-container {
  padding: 0;
}


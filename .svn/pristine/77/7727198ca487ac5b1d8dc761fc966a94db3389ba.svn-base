  -ms-box-shadow: 0 0 10px rgba(0, 0, 0, 0.7);
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.7);
  font-family: 'Titillium Web', sans-serif;
  font-size: 1.2rem;
  padding-top: 0;
  padding-bottom: 0;
  padding-left: 0;
}

.icon-menu{

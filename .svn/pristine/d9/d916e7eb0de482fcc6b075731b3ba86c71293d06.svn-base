.tableDV{
  width: 99%;
  margin: 10px 10px ;
  background-color: #ffffff;
  border-radius: 10px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
}

.tableRHDV{
  background-color: #f5f5f5;
  border-radius: 10px 10px 0 0;
}

.tableRDV{ }


.tableRDV:nth-child(odd) { background-color: #f5f5f5; }
.tableRDV:nth-child(even) { background-color: #ffffff; }
.tableRDV:last-child { border-radius: 0 0 10px 10px; }


.tableHDV{
  width: 85px;
  color: #333;
  margin: 10px;
  padding: 5px 0 5px 0;
  text-align: center;
  font-size: 16px;
  font-weight: 600;
  line-height: 1;
  vertical-align: middle;
}

.tableItem{
  width: 85px;
  color: #333;
  margin: 10px;
  padding: 10px 5px 7px 5px;
  text-align: center;
  font-size: 16px;
  font-weight: 400;
  line-height: 1;
  vertical-align: middle;
}




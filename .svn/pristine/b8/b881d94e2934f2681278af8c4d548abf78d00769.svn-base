
<div class="container-fluid">
  <div class="row">

    <div class="col-sm-12 col-md-12 col-lg-6 col-xl-6 lista " id="grid-wrapper">

      <ul class="listaPadre">
        <li class="tituloLista">
          <ul class="rowItem">
            <li class="titulopdelalista"><h5>Titulo de la Lista de informes</h5></li>
            <li class="verMasboton"><p>Ver Más</p></li>
          </ul>
        </li>

        <li class="itemLista" *ngFor="let fav of favoritos">
          <ul class="rowItem">
            <li class="estrellainput"><input class="star"  type="checkbox"></li>
            <li class="titulodeImforme">{{ fav.tituloinforme }}</li>
            <li class="fechaLista">{{ fav.fechas }}</li>
            <li><img class="icon-lista" [src]="fav.iconoPDF"> </li>
            <li><input type="checkbox"></li>
          </ul>
        </li>

      </ul>
    </div>

    <div class="col-sm-12 col-md-12 col-lg-6 col-xl-6 lista no-left-gutters">

      <ul class="listaPadre">
        <li class="tituloLista">
          <ul class="rowItem">
            <li class="titulopdelalista"><h5>Titulo de la Lista de informes</h5></li>
            <li class="verMasboton"><p>Ver Más</p></li>
          </ul>
        </li>

        <li class="itemLista" *ngFor="let fav of favoritos">
          <ul class="rowItem">
            <li class="estrellainput"><input class="star"  type="checkbox"></li>
            <li class="titulodeImforme">{{ fav.tituloinforme }}</li>
            <li class="fechaLista">{{ fav.fechas }}</li>
            <li><img class="icon-lista" [src]="fav.iconoPDF"> </li>
            <li><input type="checkbox"></li>
          </ul>
        </li>

      </ul>
    </div>

  </div>
</div>


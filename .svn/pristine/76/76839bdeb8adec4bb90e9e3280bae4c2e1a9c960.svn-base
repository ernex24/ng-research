/*NUEVOS ESTILOS BOTONES BOOTSTRAP sobre escribir estilos de formulario bootstrap*/
/*chips*/

.gvc-button-group {
  display: block;
}

.gvc-button-chip {
  padding: 8px 10px;
  margin: 12px;
  border: 1px solid #F6F7FA;
  color: #666666;
  background: #F6F7FA;
  text-align: center;
  cursor: pointer;
  text-transform: uppercase;
  font-size: 14px;
  margin-bottom: 0;
  border-radius: 20px 20px 20px 20px !important;
  -webkit-transition: 0.2s ease-out;
  transition: 0.2s ease-out;
}

.gvc-button-chip.active {
  padding: 8px 10px;
  margin: 12px;
  border: 1px solid #41CBD2;
  color: #FFFFFF;
  text-align: center;
  font-size: 14px;
  cursor: pointer;
  margin-bottom: 0;
  text-transform: uppercase;
  border-radius: 20px 20px 20px 20px !important;
  -webkit-transition: 0.2s ease-out;
  transition: 0.2s ease-out;
  background: #41cbd2;
  background: linear-gradient(135deg, #41cbd2 0%, #109ba2 100%);
}

.gvc-button-chip:hover {
  color: #FFFFFF;
  background: #109BA2;
  border: 1px solid #41CBD2;
  margin-bottom: 0;
}

.gvc-button-chip:hover.active {
  color: #FFFFFF;
  border: 1px solid #41CBD2;
  margin-bottom: 0;
  background: #41cbd2;
  background: linear-gradient(135deg, #41cbd2 0%, #109ba2 100%);
}


@charset "UTF-8";

/*Carga de tipografias de google */
@import url("https://fonts.googleapis.com/css?family=Titillium+Web:400,600,700");
@import url("https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700");

/* ==========================================================================
   Estilos Base Generales
   ========================================================================== */

body,
html {
  height: 100%;
  margin: 0;
  font-size: 16px;
  line-height: 24px;
}

ul {
  padding: 0;
}

hr {
  padding: 0;
  margin: 5px auto;
  width: 200px;
}

a {
  color: #FFFFFF !important;
  cursor: pointer;
}

a:hover {
  color: #09595d;
  text-decoration: none;
}

a:active {
  color: #41CBD2 !important;
  text-decoration: none;
}

ul li a:active {
  color: #333333 !important;
  text-decoration: none;
}

.margin15 {
  margin: 0 15px;
}

.marginTop15{
  margin: 15px 0 0 0;
}

/*Estilos Scroll Bar General toda la app */

::-webkit-scrollbar-track {
  box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
  -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
  background-color: #eeeeee;
  border-radius: 3px;
}

::-webkit-scrollbar {
  height: 6px;
  width: 3px;
  background-color: #F5F5F5;
  border-radius: 3px;
}

::-webkit-scrollbar-thumb {
  background-color: #009CA4;
}

/* Spinner general */

#preLoader {
  display: block;
  background-image: url(./assets/img/imagenes_comunes/preLoader/rings.svg);
  background-size: 150px 150px;
  background-repeat: no-repeat;
  background-position: center center;
  width: 150px;
  height: 150px;
  margin: 0 auto;
  background-color: rgba(255, 255, 255, .5);
  opacity: 1;
  -webkit-transition: all 1s ease-in-out;
  transition: all 1s ease-in-out
}

.spinner {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -100;
  background: #222222;
  overflow: hidden;
}

.spinner svg {
  width: 200px;
  display: block;
  margin: 15% auto;
}

/* ==========================================================================
   Estilos sobre escritos de Bootstrap 4 modificaciones sobre el layout
   ========================================================================== */

.row .no-gutters {
  margin-right: 0;
  margin-left: 0;
  padding-right: 0;
  padding-left: 0;
}

 .no-left-gutters {
  margin-left: 0;
  padding-left: 0;
}

ul,
ol {
  list-style-type: none;
}

.gvc-link-hover {
  background-color: #41CBD2;
}

/* ==========================================================================
    Estilos generales de tipografias
   ========================================================================== */

/*Typografia*/

h1 {
  margin: 0;
  font-family: "Titillium Web", sans-serif;
  font-size: 2em;
}

h2 {
  margin: 0;
  font-family: "Titillium Web", sans-serif;
  font-size: 0.875em;
  font-weight: 400;
}

h3 {
  margin: 0;
  font-family: "Titillium Web", sans-serif;
  font-size: 0.875em;
  text-transform: uppercase;
}

h4 {
  margin: 0;
  font-family: "Titillium Web", sans-serif;
  font-size: 0.875em;
}

h5 {
  margin: 0;
  font-family: "Open Sans", sans-serif;
  font-size: 1em;
  font-weight: 700;
}

h6 {

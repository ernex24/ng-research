/* ==========================================================================
   Clases Menu Perfil
   ========================================================================== */

.my-modal {
  left: 0;
  z-index: 20000;
  position: fixed;
  border: 0;
  width: 100%;
  height: 100%;
  background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.9) 0%, rgba(15, 171, 179, 0.6) 100%);
}

.menuProfileItem {
  max-width: 300px;
  line-height: 2;
  -webkit-transition: all 0.3s;
  transition: all 0.3s;
  font-size: 18px;
  border-radius: 10px;
  margin: 0 auto;
  font-family: "Open Sans", sans-serif;
}

.menuProfileItem:hover {
  background-color: rgba(255, 255, 255, 0.2);
  font-size: 20px;
  line-height: 2;
  -webkit-transition: all 0.3s;
  transition: all 0.3s;
  border-radius: 10px;
  -webkit-box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
}

.menuProfileItem:active a {
  color: #333333 !important;
}

.menuProfileItem:active {
  background-color: rgba(255, 255, 255, 0.8);
  color: #333333 !important;
}


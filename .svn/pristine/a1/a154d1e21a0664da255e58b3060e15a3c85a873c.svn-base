/* ==========================================================================
    Nav Header
   ========================================================================== */

.menuPrincipal{
  background-image: -moz-linear-gradient( 0deg, rgb(15,171,179) 0%, rgb(16,155,162) 100%, rgb(16,155,162) 100%);
  background-image: -webkit-linear-gradient( 0deg, rgb(15,171,179) 0%, rgb(16,155,162) 100%, rgb(16,155,162) 100%);
  background-image: -ms-linear-gradient( 0deg, rgb(15,171,179) 0%, rgb(16,155,162) 100%, rgb(16,155,162) 100%);
  box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 50px;
  font-size: 16px;
}

.colorBarra{
  color: #FFFFFF !important;
  background: #41cbd2;
  background: linear-gradient(135deg, #41cbd2 0%, #109ba2 100%);
  -webkit-box-shadow: 0 0 10px rgba(0, 0, 0, 0.7);
  -ms-box-shadow: 0 0 10px rgba(0, 0, 0, 0.7);
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.7);
  font-family: 'Titillium Web', sans-serif;
  font-size: 1.2rem;
  padding-top: 4px;
  padding-bottom: 4px;
  z-index: 15;
}


.menuPrincipal img {
  margin-top:-4px;
}

.nombreSeccion{
  position: absolute;
  display: inline-flex;
  color: #ffffff;
  text-transform: uppercase;
  top: 9px;
  padding-left: 0;
}

.tituloSeccion{
  padding: 8px 10px;
}

ul.menuSecciones{
  position: absolute;
  display: flex;
  right: 10px;
}

ul.menuSecciones li{
  margin: 5px 0 0 40px;
}

.user img{
  padding-left: 10px;
}

.user::after{
 /* background-image: url('../../../assets/img/imagenes_comunes/interfaz/arrow.png'); */
  background-repeat:no-repeat;
  background-position: left center;
}

.userName{
  font-weight: 600;
  margin-right: 10px;
}

.icon-menu{
  width: 30px;
}

.badge-secondary {
  color: #fff;
  background-color: #ff3554;
}

.companyName{
  background-color: #0C8C92;
  padding:6px 6px;
  border-radius: 10px;
  font-size: 12px;
  text-transform: uppercase;
  font-weight: 800;
  margin: 10px;
}

/* ==========================================================================
    Media Queries
   ========================================================================== */

/* Extra small devices (portrait phones, less than 576px)*/
@media (max-width: 575px) {

  .formulario{
    width: 100% !important;
    transition: all 250ms ease-in-out;
    margin-top: 10px;
  }

  .sug-wraper {

    display: none;
    width: 335px;
    transition: all 250ms ease-in-out;

  }

  .tab-wraper {

    display: none;
    width: 335px;
    transition: all 250ms ease-in-out;

  }

  .navbar-nav {
    flex-direction: row !important;
    float: left;
  }

  .navbar-nav li img{
    margin-right: 15px ;
  }


  #sidebar-wrapperG {
    display: none;
    transition: all 0.4s ease 0s;
  }

  #wrapper {
    padding-left: 0;
    transition: all 0.4s ease 0s;
  }

  .menuLateral2{
    background-image: -webkit-linear-gradient(165deg, rgb(0,0,0) 0%, rgb(29,29,29) 100%);
    background-image: -o-linear-gradient(165deg, rgb(0,0,0) 0%, rgb(29,29,29) 100%);
    background-image: linear-gradient(255deg, rgb(0,0,0) 0%, rgb(29,29,29) 100%);
    box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.5);
    display: block;
    margin: 10px 0 10px 0;
    width: 100%;
    left: 0;
    transition: all 250ms ease-in-out;
    color: #fff;
    border-radius: 10px;
  }

  .menuLateral2 li{
    padding-left: 80px;
    padding-top: 15px;
    padding-bottom: 15px;
    color: #fff !important;
  }

  .menuLateral2 li a{
    color: #fff !important;

  }

  .rrss{
    margin-left: -40px;
    transition: all 250ms ease-in-out;
  }

  .rrss img{
    padding-right: 30px;
    transition: all 250ms ease-in-out;
  }

}

/* Small devices (landscape phones, 576px and up)*/
@media (min-width: 576px) and (max-width: 768px) {

  .formulario{
    width: 100% !important;
    transition: all 250ms ease-in-out;
    margin-top: 10px;
  }

  .sug-wraper {
    display: none;
    width: 512px;
    transition: all 250ms ease-in-out;
  }

  .tab-wraper {
    display: none;
    width: 512px;
    transition: all 250ms ease-in-out;
  }

  .navbar-nav {
    flex-direction: row !important;
    float: right;
    transition: all 250ms ease-in-out;
  }
  .navbar-nav li img{
    margin-left: 20px;
    transition: all 250ms ease-in-out;
  }

  #sidebar-wrapperG {
    display: none;
    transition: all 250ms ease-in-out;
  }

  #wrapper {
    padding-left: 0;
    transition: all 250ms ease-in-out;
  }

  .menuLateral2{
    background-image: -webkit-linear-gradient(165deg, rgb(0,0,0) 0%, rgb(29,29,29) 100%);
    background-image: -o-linear-gradient(165deg, rgb(0,0,0) 0%, rgb(29,29,29) 100%);
    background-image: linear-gradient(255deg, rgb(0,0,0) 0%, rgb(29,29,29) 100%);
    box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.5);
    display: block;
    margin: 10px 0 10px 0;
    width: 100%;
    left: 0;
    transition: all 250ms ease-in-out;
    border-radius: 10px;
  }

  .menuLateral2 li{
    padding-left: 80px;
    padding-top: 15px;
    padding-bottom: 15px;
    transition: all 250ms ease-in-out;
    border-bottom: 1px solid #222222;
  }

  .menuLateral2 li:nth-child(5) {
    border-bottom: none;
  }

  .menuLateral2 li a{
    color: #fff !important;
  }

  .rrss{
    margin-left: -76px;
  }

  .rrss img{
    padding-right: 50px;
  }

  .userName {
    font-weight: 600;
    transition: all 250ms ease-in-out;
  }

}

/* Medium devices (tablets, 768px and up)*/
@media (min-width: 769px) and (max-width: 991px) {

  .formulario{
    width: 100% !important;
    transition: all 250ms ease-in-out;
    margin-top: 10px;
  }

  .sug-wraper {
    display: none;
    width: 690px;
    transition: all 250ms ease-in-out;
  }

  .tab-wraper {
    display: none;
    width: 690px;
    transition: all 250ms ease-in-out;
  }

  .navbar-nav {
    flex-direction: row !important;
    float: right;
    transition: all 250ms ease-in-out;
  }

  .navbar-nav li{
    transition: all 250ms ease-in-out;
  }

  .menuLateral2{
    display: none;
  }

}

/* Large devices (desktops, 992px and up)*/
@media (min-width: 992px) and (max-width: 1199px) {

  .formulario{
    width: 380px !important;
    transition: all 250ms ease-in-out;
  }

  .sug-wraper {
    display: none;
    transition: all 250ms ease-in-out;
    width: 350px;
  }

  .tab-wraper {
    display: none;
    transition: all 250ms ease-in-out;
    width: 350px;
  }

  .navbar-nav {
    flex-direction: row !important;
    float: right;
    transition: all 250ms ease-in-out;
  }

  .menuLateral2{
    display: none;
  }

}


/* Extra large devices (large desktops, 1200px and up)*/
@media (min-width: 1200px) {

  .menuLateral2{
    display: none;
  }

}



<div >
  <ul class="listaPadre">
    <li class="tituloLista">
      <ul class="rowItem">
        <li class="titulopdelalista"><h5>Blog</h5></li>
      </ul>
    </li>

    <li class="itemLista" *ngFor=" let posts of Blogarticulos">
      <ul class="columnItem">
        <li class="seccionNoticia">{{ posts.seccionDelBlog }}</li>
        <li class="tituloNoticia"><h5>{{ posts.tituloArticuloBlog }}</h5></li>
        <li class="fechaNoticia">{{ posts.fechaArticuloBlog }}</li>
        <li class="contenidoNoticia">{{ posts.articuloBlog }}</li>
      </ul>
    </li>

  </ul>
</div>

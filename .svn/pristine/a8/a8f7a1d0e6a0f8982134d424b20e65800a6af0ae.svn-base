
.my-modal {
  border: 0;
  width: 100%;
  height: 100%;
  background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.88) 0%, rgba(15, 171, 179, 0.4) 100%);
}

.modal-dialog{
  max-width: 100%;
  margin: 0px;
  color: rgba(0, 0, 0, 0.0);
}

.modal-content{
  background-color: rgba(0, 0, 0, 0.0);
  border: 0;
  max-width: 100%;
}
